/*
 *  Character duplication in string MAP, Solution O(N)
 *
 */

#include <iostream>
#include <string>
#include <conio.h>
#include <map>
using namespace std;

bool dupCharacterCheck(string temp)
{
	map<char, bool> m1;
	if (temp.length() == 0)
		return false;
	for (int i = 0; i < temp.length(); i++)
	{
		if (m1.find(temp[i]) != m1.end())
		{
			m1[temp[i]] = true;
		}
		else
			return true;
	}
	return false;
}

int main()
{
	string temp = "Tanuja";
	bool check = false;
	check = dupCharacterCheck(temp);
	if (check)
		cout << "Duplicate Character Present";
	else
		cout << "Not Present";
	_getch();
}