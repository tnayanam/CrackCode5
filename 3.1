/*
*  Check if given string is palindrome, Solution O(N square)
*
*/

bool checkPermutation(string temp1, string temp2)
{
	bool flag = false;
	if (temp1.length() != temp2.length())
		return false;
	for (int i = 0; i < temp1.length(); i++)
	{
		flag = false;
		for (int j = 0; j < temp2.length(); j++)
		{
			if (temp1[i] == temp2[j])
			{
				flag = true;
				break;
			}
		}
		if (!flag)
			return false;
	}
	return flag;
}

int main()
{
	string temp1 = "qTanuj";
	string temp2 = "naujTp";
	checkPermutation(temp1, temp2) ? cout << "Yes palindrome" : cout << "No Palindrome";
	_getch();
}
